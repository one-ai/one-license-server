{
    "name": "express-gen-ts",
    "version": "0.0.0",
    "scripts": {
        "build": "rm -rf dist && rm -rf frontend/dist && rm -rf app/public && cd frontend && yarn build && cd .. && mv frontend/build app/public && ts-node ./util/build.ts",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "start": "node -r module-alias/register ./dist",
        "start:app-dev": "nodemon --config nodemon.json",
        "start:front-dev": "cd frontend && yarn start",
        "test": "nodemon --config nodemon.test.json",
        "install": "cd frontend && npm i && cd .."
    },
    "husky": {
        "pre-commit": "npm run lint"
    },
    "_moduleAliases": {
        "@daos": "dist/daos",
        "@entities": "dist/entities",
        "@shared": "dist/shared",
        "@server": "dist/Server",
        "@config": "dist/config"
    },
    "dependencies": {
        "command-line-args": "^5.1.1",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "helmet": "^3.22.0",
        "http-status-codes": "^1.4.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "tslib": "^1.11.1",
        "winston": "^3.2.1",
        "@types/command-line-args": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.3",
        "@types/find": "^0.2.1",
        "@types/helmet": "0.0.45",
        "@types/jasmine": "^3.5.10",
        "@types/jsonfile": "^5.0.0",
        "@types/morgan": "^1.9.0",
        "@types/node": "^13.9.5",
        "@types/supertest": "^2.0.8",
        "find": "^0.3.0",
        "jasmine": "^3.5.0",
        "jsonfile": "^6.0.1",
        "nodemon": "^2.0.2",
        "react-scripts": "^3.4.1",
        "supertest": "^4.0.2",
        "ts-node": "^8.8.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^6.1.0",
        "typescript": "^3.8.3"
    },
    "engines": {
        "yarn": "^1.22.4",
        "node": "^12.16.1",
        "npm": "6.13.4"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.1.0",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "fs-extra": "^9.0.0",
        "husky": "^4.2.3",
        "prettier": "^2.0.2"
    }
}
